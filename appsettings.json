{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=0.0.0.0;Database=YourDatabase;User Id=User;Password=Password;TrustServerCertificate=True;"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secure-secret-key-min-256-bits-long",
    "Issuer": "your-app",
    "Audience": "your-clients",
    "ExpirationInMinutes": 60
  }
}